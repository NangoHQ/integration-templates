[
    {
        "providerConfigKey": "calendly",
        "syncs": [
            {
                "name": "events",
                "type": "sync",
                "description": "Retrieve all events per a user",
                "sync_type": "incremental",
                "usedModels": ["Event", "EventLocation", "InviteesCounter", "EventMembership", "EventGuest", "CalendarEvent", "EventCancellation"],
                "runs": "every hour",
                "version": "2.0.0",
                "track_deletes": false,
                "auto_start": true,
                "input": null,
                "output": ["Event"],
                "scopes": [],
                "endpoints": [
                    {
                        "method": "GET",
                        "path": "/events"
                    }
                ],
                "webhookSubscriptions": []
            },
            {
                "name": "event-types",
                "type": "sync",
                "description": "Retrieve all event types per a user",
                "sync_type": "incremental",
                "usedModels": ["EventType", "EventProfile", "EventTypeLocation", "EventQuestion"],
                "runs": "every hour",
                "version": "2.0.0",
                "track_deletes": false,
                "auto_start": true,
                "input": null,
                "output": ["EventType"],
                "scopes": [],
                "endpoints": [
                    {
                        "method": "GET",
                        "path": "/event/types"
                    }
                ],
                "webhookSubscriptions": []
            },
            {
                "name": "event-invitees",
                "type": "sync",
                "description": "For all events (active and canceled) retrieve the event invitees",
                "sync_type": "incremental",
                "usedModels": ["EventInvitee", "QuestionAndAnswer", "Tracking", "EventCancellation", "Payment", "Reconfirmation"],
                "runs": "every hour",
                "version": "1.0.0",
                "track_deletes": false,
                "auto_start": true,
                "input": null,
                "output": ["EventInvitee"],
                "scopes": [],
                "endpoints": [
                    {
                        "method": "GET",
                        "path": "/event/invitees"
                    }
                ],
                "webhookSubscriptions": []
            },
            {
                "name": "users",
                "type": "sync",
                "description": "Fetches a list of users from Calendly",
                "sync_type": "full",
                "usedModels": ["User"],
                "runs": "every day",
                "version": "2.0.0",
                "track_deletes": true,
                "auto_start": true,
                "input": null,
                "output": ["User"],
                "scopes": [],
                "endpoints": [
                    {
                        "method": "GET",
                        "path": "/users",
                        "group": "Users"
                    }
                ],
                "webhookSubscriptions": []
            }
        ],
        "actions": [
            {
                "name": "create-user",
                "type": "action",
                "description": "Creates a user in Calendly",
                "version": "1.0.0",
                "scopes": ["admin"],
                "input": "CreateUser",
                "output": ["User"],
                "usedModels": ["User", "CreateUser"],
                "endpoint": {
                    "method": "POST",
                    "path": "/users",
                    "group": "Users"
                }
            },
            {
                "name": "delete-user",
                "type": "action",
                "description": "Deletes a user in Calendly",
                "version": "1.0.0",
                "scopes": ["admin"],
                "input": "IdEntity",
                "output": ["SuccessResponse"],
                "usedModels": ["SuccessResponse", "IdEntity"],
                "endpoint": {
                    "method": "DELETE",
                    "path": "/users",
                    "group": "Users"
                }
            },
            {
                "name": "whoami",
                "type": "action",
                "description": "Fetch current user information",
                "version": "1.0.0",
                "scopes": [],
                "input": null,
                "output": ["UserInformation"],
                "usedModels": ["UserInformation"],
                "endpoint": {
                    "method": "GET",
                    "path": "/whoami",
                    "group": "Users"
                }
            }
        ],
        "onEventScripts": {
            "post-connection-creation": [],
            "pre-connection-deletion": []
        }
    }
]
