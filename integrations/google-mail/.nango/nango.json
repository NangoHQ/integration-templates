    "syncs": [
      {
        "type": "sync",
        "description": "Fetches a list of emails from gmail. Goes back default to 1 year\nbut metadata can be set using the `backfillPeriodMs` property\nto change the lookback. The property should be set in milliseconds.",
        "auto_start": true,
        "endpoints": [
          {
            "method": "GET",
            "path": "/emails",
            "group": "Emails"
          }
        ],
        "input": "SyncMetadata_google_mail_emails",
        "name": "emails",
        "output": [
          "GmailEmail"
        ],
        "runs": "every hour",
        "scopes": [
          "https://www.googleapis.com/auth/gmail.readonly"
        ],
        "sync_type": "incremental",
        "track_deletes": false,
        "usedModels": [
          "GmailEmail",
          "SyncMetadata_google_mail_emails"
        ],
        "version": "2.0.1",
        "webhookSubscriptions": []
      },
      {
        "type": "sync",
        "description": "Fetches a list of labels from gmail.",
        "auto_start": true,
        "endpoints": [
          {
            "method": "GET",
            "path": "/labels",
            "group": "Labels"
          }
        ],
        "input": "SyncMetadata_google_mail_labels",
        "name": "labels",
        "output": [
          "GmailLabel"
        ],
        "runs": "every 6 hours",
        "scopes": [
          "https://www.googleapis.com/auth/gmail.readonly"
