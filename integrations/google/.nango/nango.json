        "description": "Sync all workspace org units",
        "auto_start": true,
        "endpoints": [
          {
            "method": "GET",
            "path": "/google/workspace-org-unit"
          }
        ],
        "input": "SyncMetadata_google_workspaceorgunits",
        "name": "workspace-org-units",
        "output": [
          "OrganizationalUnit"
