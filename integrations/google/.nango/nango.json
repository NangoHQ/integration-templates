        "scopes": [
          "https://www.googleapis.com/auth/admin.directory.orgunit.readonly",
          "https://www.googleapis.com/auth/admin.directory.user.readonly"
        ],
        "sync_type": "full",
        "track_deletes": false,
        "usedModels": [
          "OrganizationalUnit",
          "SyncMetadata_google_workspaceorgunits"
        ],
        "version": "2.0.0",
        "webhookSubscriptions": []
      },
      {
