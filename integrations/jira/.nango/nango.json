[
  {
    "providerConfigKey": "jira",
    "actions": [
      {
        "type": "action",
        "description": "An action that creates an Issue on Jira",
        "endpoint": {
          "method": "POST",
          "path": "/issues",
          "group": "Issues"
        },
        "input": "ActionInput_jira_createissue",
        "name": "create-issue",
        "output": [
          "ActionOutput_jira_createissue"
        ],
        "scopes": [
          "write:jira-work"
        ],
        "usedModels": [
          "ActionInput_jira_createissue",
          "ActionOutput_jira_createissue"
        ],
        "version": "2.0.0"
      }
    ],
    "syncs": [
      {
        "type": "sync",
        "description": "Fetches a list of issues from Jira",
        "auto_start": false,
        "endpoints": [
          {
            "method": "GET",
            "path": "/issues",
            "group": "Issues"
          }
        ],
        "input": "SyncMetadata_jira_issues",
        "name": "issues",
        "output": [
          "Issue"
        ],
        "runs": "every 5mins",
        "scopes": [
          "read:jira-work"
        ],
        "sync_type": "incremental",
        "track_deletes": false,
        "usedModels": [
          "Issue",
          "SyncMetadata_jira_issues"
        ],
        "version": "2.0.1",
        "webhookSubscriptions": []
      },
      {
        "type": "sync",
        "description": "Fetches a list of projects from Jira",
        "auto_start": true,
        "endpoints": [
          {
            "method": "GET",
            "path": "/projects"
          }
        ],
        "input": "SyncMetadata_jira_projects",
        "name": "projects",
        "output": [
          "Project"
        ],
        "runs": "every day",
        "scopes": [
          "read:jira-work"
        ],
        "sync_type": "full",
        "track_deletes": false,
        "usedModels": [
          "Project",
          "SyncMetadata_jira_projects"
        ],
        "version": "1.0.0",
        "webhookSubscriptions": []
      },
      {
        "type": "sync",
        "description": "Fetches a list of issue types for a project",
        "auto_start": false,
        "endpoints": [
          {
            "method": "GET",
            "path": "/issue-types"
          }
        ],
        "input": "SyncMetadata_jira_issuetypes",
        "name": "issue-types",
        "output": [
          "IssueType"
        ],
        "runs": "every day",
        "scopes": [
          "read:jira-work"
        ],
        "sync_type": "full",
        "track_deletes": false,
        "usedModels": [
          "IssueType",
          "SyncMetadata_jira_issuetypes"
        ],
        "version": "2.0.0",
        "webhookSubscriptions": []
      }
    ],
    "onEventScripts": {
