[
  {
    "providerConfigKey": "gong",
    "actions": [
      {
        "type": "action",
        "description": "Fetches a list of call transcripts from Gong",
        "endpoint": {
          "method": "GET",
          "path": "/fetch-call-transcripts",
          "group": "Calls"
        },
        "input": "ActionInput_gong_fetchcalltranscripts",
        "name": "fetch-call-transcripts",
        "output": [
          "ActionOutput_gong_fetchcalltranscripts"
        ],
        "scopes": [
          "api:calls:read:transcript"
        ],
        "usedModels": [
          "ActionInput_gong_fetchcalltranscripts",
          "ActionOutput_gong_fetchcalltranscripts"
        ],
        "version": "2.0.0"
      }
    ],
    "syncs": [
      {
        "type": "sync",
        "description": "Fetches a list of call transcripts from Gong",
        "auto_start": true,
        "endpoints": [
          {
            "method": "GET",
            "path": "/call-transcripts",
            "group": "Calls"
          }
        ],
        "input": "SyncMetadata_gong_calltranscripts",
        "name": "call-transcripts",
        "output": [
          "GongCallTranscriptSyncOutput"
        ],
        "runs": "every 1h",
        "scopes": [
          "api:calls:read:transcript"
        ],
        "sync_type": "incremental",
        "track_deletes": false,
        "usedModels": [
          "GongCallTranscriptSyncOutput",
          "SyncMetadata_gong_calltranscripts"
        ],
        "version": "2.0.0",
        "webhookSubscriptions": []
      },
      {
        "type": "sync",
        "description": "Fetches a list of calls from Gong",
        "auto_start": true,
        "endpoints": [
          {
            "method": "GET",
            "path": "/calls",
            "group": "Calls"
          }
        ],
        "input": "SyncMetadata_gong_calls",
        "name": "calls",
        "output": [
          "GongCallOutput"
        ],
        "runs": "every 1h",
        "scopes": [
          "api:calls:read:basic",
          "api:calls:read:media-url",
          "api:calls:read:extensive"
        ],
        "sync_type": "incremental",
        "track_deletes": false,
        "usedModels": [
          "GongCallOutput",
          "SyncMetadata_gong_calls"
        ],
        "version": "2.0.0",
        "webhookSubscriptions": []
      },
      {
        "type": "sync",
        "description": "Fetches the list of gong users",
        "auto_start": true,
        "endpoints": [
          {
            "method": "GET",
            "path": "/users",
            "group": "Users"
          }
        ],
        "input": "SyncMetadata_gong_users",
        "name": "users",
        "output": [
          "User"
        ],
        "runs": "every day",
        "scopes": [
          "api:users:read"
        ],
        "sync_type": "incremental",
        "track_deletes": false,
        "usedModels": [
          "User",
          "SyncMetadata_gong_users"
        ],
        "version": "2.0.0",
        "webhookSubscriptions": []
      }
    ],
    "onEventScripts": {
      "post-connection-creation": [],
      "pre-connection-deletion": [],
      "validate-connection": []
    }
  }
]