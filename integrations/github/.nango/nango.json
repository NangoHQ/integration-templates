[
  {
    "providerConfigKey": "github",
    "actions": [
      {
        "type": "action",
        "description": "Write content to a particular github file within a repo. If\nthe file doesn't exist it creates and then writes to it",
        "endpoint": {
          "method": "PUT",
          "path": "/files",
          "group": "Files"
        },
        "input": "ActionInput_github_writefile",
        "name": "write-file",
        "output": [
          "ActionOutput_github_writefile"
        ],
        "scopes": [
          "repo"
        ],
        "usedModels": [
          "ActionInput_github_writefile",
          "ActionOutput_github_writefile"
        ],
        "version": "2.0.0"
      },
      {
        "type": "action",
        "description": "List github repos from an organization.",
        "endpoint": {
          "method": "GET",
          "path": "/github/list-repos"
        },
        "input": "ActionInput_github_listrepos",
        "name": "list-repos",
        "output": [
          "ActionOutput_github_listrepos"
        ],
        "scopes": [
          "read:org"
        ],
        "usedModels": [
          "ActionInput_github_listrepos",
          "ActionOutput_github_listrepos"
        ],
        "version": "2.0.0"
      }
    ],
    "syncs": [
      {
        "type": "sync",
        "description": "Fetches Github issues from all a user's repositories",
        "auto_start": true,
        "endpoints": [
          {
            "method": "GET",
            "path": "/github/issues"
          }
        ],
        "input": "SyncMetadata_github_issues",
        "name": "issues",
        "output": [
          "GithubIssue"
        ],
        "runs": "every half hour",
        "scopes": [
          "public_repo"
        ],
        "sync_type": "full",
        "track_deletes": false,
        "usedModels": [
          "GithubIssue",
          "SyncMetadata_github_issues"
        ],
        "version": "2.0.0",
        "webhookSubscriptions": []
      },
      {
        "type": "sync",
        "description": "Fetches Github issues but up to a maximum of 15",
        "auto_start": false,
        "endpoints": [
          {
            "method": "GET",
            "path": "/github/issues-lite"
          }
        ],
        "input": "SyncMetadata_github_issueslite",
        "name": "issues-lite",
        "output": [
          "Issue"
        ],
        "runs": "every day",
        "scopes": [
          "public_repo"
        ],
        "sync_type": "full",
        "track_deletes": false,
        "usedModels": [
          "Issue",
          "SyncMetadata_github_issueslite"
        ],
        "version": "2.0.0",
        "webhookSubscriptions": []
      },
      {
        "type": "sync",
        "description": "Lists all the files of a Github repo given a specific branch",
        "auto_start": false,
        "endpoints": [
          {
            "method": "GET",
            "path": "/files",
            "group": "Files"
          }
        ],
        "input": "SyncMetadata_github_listfiles",
        "name": "list-files",
        "output": [
          "GithubRepoFile"
        ],
        "runs": "every hour",
        "scopes": [
          "repo"
        ],
        "sync_type": "incremental",
        "track_deletes": false,
        "usedModels": [
          "GithubRepoFile",
          "SyncMetadata_github_listfiles"
        ],
        "version": "2.0.1",
        "webhookSubscriptions": []
      }
    ],
    "onEventScripts": {
      "post-connection-creation": [],
      "pre-connection-deletion": [],
      "validate-connection": []
    }
  }
]