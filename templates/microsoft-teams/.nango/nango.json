[
  {
    "providerConfigKey": "microsoft-teams",
    "actions": [],
    "syncs": [
      {
        "type": "sync",
        "description": "Continuously fetches groups from either Microsoft 365 or Azure Active\nDirectory.\nDetails: full refresh, support deletes, goes back all time, metadata\nis not required.",
        "auto_start": true,
        "endpoints": [
          {
            "method": "GET",
            "path": "/org-units",
            "group": "Org Units"
          }
        ],
        "input": "SyncMetadata_microsoft_teams_orgunits",
        "name": "org-units",
        "output": [
          "OrganizationalUnit"
        ],
        "runs": "every 6 hours",
        "scopes": [
          "GroupMember.Read.All"
        ],
        "sync_type": "full",
        "track_deletes": false,
        "usedModels": [
          "OrganizationalUnit",
          "SyncMetadata_microsoft_teams_orgunits"
        ],
        "version": "1.0.3",
        "webhookSubscriptions": []
      },
      {
        "type": "sync",
        "description": "Continuously fetches users from either Microsoft 365 or Azure Active\nDirectory given specified\ngroups to sync.",
        "auto_start": false,
        "endpoints": [
          {
            "method": "GET",
            "path": "/users",
            "group": "Users"
          }
        ],
        "input": "SyncMetadata_microsoft_teams_users",
        "name": "users",
        "output": [
          "User"
        ],
        "runs": "every hour",
        "scopes": [
          "User.Read.All"
        ],
        "sync_type": "full",
        "track_deletes": false,
        "usedModels": [
          "User",
          "SyncMetadata_microsoft_teams_users"
        ],
        "version": "1.0.3",
        "webhookSubscriptions": []
      },
      {
        "type": "sync",
        "description": "Continuously fetches messages from Microsoft Teams channels and chats.\nDetails: incremental sync, goes back 10 days on first sync, metadata tracks last sync per channel/chat.",
        "auto_start": true,
        "endpoints": [
          {
            "method": "GET",
            "path": "/messages",
            "group": "Messsages"
          }
        ],
        "input": "SyncMetadata_microsoft_teams_messages",
        "name": "messages",
        "output": [
          "TeamsMessage"
        ],
        "runs": "every hour",
        "scopes": [
          "ChannelMessage.Read.All",
          "Chat.Read.All"
        ],
        "sync_type": "incremental",
        "track_deletes": false,
        "usedModels": [
          "TeamsMessage",
          "SyncMetadata_microsoft_teams_messages"
        ],
        "version": "0.0.1",
        "webhookSubscriptions": []
      }
    ],
    "onEventScripts": {
      "post-connection-creation": [],
      "pre-connection-deletion": []
    }
  }
]