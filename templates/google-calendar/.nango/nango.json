[
    {
        "providerConfigKey": "google-calendar",
        "syncs": [
            {
                "name": "events",
                "type": "sync",
                "description": "Sync calendar events on the primary calendar going back one month and\nsave the entire object as specified by the Google API",
                "sync_type": "incremental",
                "usedModels": ["GoogleCalendarEvent", "CalendarUser", "EventDateTime", "CalendarMetadata"],
                "runs": "every 5 minutes",
                "version": "2.0.0",
                "track_deletes": false,
                "auto_start": true,
                "input": "CalendarMetadata",
                "output": ["GoogleCalendarEvent"],
                "scopes": ["https://www.googleapis.com/auth/calendar.readonly"],
                "endpoints": [
                    {
                        "method": "GET",
                        "path": "/events",
                        "group": "Events"
                    }
                ],
                "webhookSubscriptions": []
            },
            {
                "name": "calendars",
                "type": "sync",
                "description": "Sync the calendars list of the user",
                "sync_type": "full",
                "usedModels": ["GoogleCalendar"],
                "runs": "every hour",
                "version": "1.0.1",
                "track_deletes": true,
                "auto_start": true,
                "input": null,
                "output": ["GoogleCalendar"],
                "scopes": ["https://www.googleapis.com/auth/calendar.readonly"],
                "endpoints": [
                    {
                        "method": "GET",
                        "path": "/calendars",
                        "group": "Calendars"
                    }
                ],
                "webhookSubscriptions": []
            }
        ],
        "actions": [
            {
                "name": "whoami",
                "type": "action",
                "description": "description: Fetch current user information",
                "version": "1.0.0",
                "scopes": [],
                "input": null,
                "output": ["UserInformation"],
                "usedModels": ["UserInformation"],
                "endpoint": {
                    "method": "GET",
                    "path": "/whoami",
                    "group": "Users"
                }
            },
            {
                "name": "settings",
                "type": "action",
                "description": "Fetch all user settings from Google Calendar",
                "version": "1.0.1",
                "scopes": ["https://www.googleapis.com/auth/calendar.settings.readonly"],
                "input": null,
                "output": ["SettingsResponse"],
                "usedModels": ["SettingsResponse", "CalendarSetting"],
                "endpoint": {
                    "method": "GET",
                    "path": "/settings",
                    "group": "Users"
                }
            }
        ],
        "onEventScripts": {
            "post-connection-creation": [],
            "pre-connection-deletion": []
        }
    }
]
