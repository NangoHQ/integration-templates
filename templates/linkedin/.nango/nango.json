[
  {
    "providerConfigKey": "linkedin",
    "actions": [
      {
        "type": "action",
        "description": "Create a linkedin post with an optional video",
        "endpoint": {
          "method": "POST",
          "path": "/videos"
        },
        "input": "ActionInput_linkedin_post",
        "name": "post",
        "output": [
          "ActionOutput_linkedin_post"
        ],
        "scopes": [
          "openid",
          "profile",
          "r_basicprofile",
          "w_member_social",
          "email",
          "w_organization_social",
          "r_organization_social"
        ],
        "usedModels": [
          "ActionInput_linkedin_post",
          "ActionOutput_linkedin_post"
        ],
        "version": "1.0.0"
      }
    ],
    "syncs": [
      {
        "type": "sync",
        "description": "This sync captures all LinkedIn messages for a Linkedin member for archiving purposes",
        "auto_start": true,
        "endpoints": [
          {
            "method": "GET",
            "path": "/messages",
            "group": "messages"
          }
        ],
        "input": "SyncMetadata_linkedin_messages",
        "name": "messages",
        "output": [
          "LinkedInMessage"
        ],
        "runs": "every 1h",
        "scopes": [
          "r_dma_portability_3rd_party"
        ],
        "sync_type": "incremental",
        "track_deletes": false,
        "usedModels": [
          "LinkedInMessage",
          "SyncMetadata_linkedin_messages"
        ],
        "version": "1.0.0",
        "webhookSubscriptions": []
      }
    ],
    "onEventScripts": {
      "post-connection-creation": [],
      "pre-connection-deletion": []
    }
  }
]