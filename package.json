{
    "name": "integration-templates",
    "version": "1.0.0",
    "type": "module",
    "description": "",
    "main": "index.js",
    "scripts": {
        "lint": "eslint . --ext .ts",
        "test": "vitest",
        "prettier-format": "prettier --config .prettierrc \"./**/*.{ts,tsx}\" --write",
        "prettier-watch": "onchange './**/*.{ts,tsx}' -- prettier --write {{changed}}",
        "prepare": "husky install"
    },
    "keywords": [],
    "license": "ISC",
    "devDependencies": {
        "ajv": "8.12.0",
        "@tsconfig/node18-strictest-esm": "1.0.1",
        "@types/node": "20.12.2",
        "@typescript-eslint/eslint-plugin": "7.7.1",
        "@typescript-eslint/parser": "7.7.1",
        "eslint": "8.56.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-import": "2.29.1",
        "eslint-plugin-prettier": "5.1.3",
        "lint-staged": "15.2.9",
        "js-yaml": "4.1.0",
        "onchange": "7.1.0",
        "husky": "8.0.3",
        "typescript": "5.3.3",
        "vitest": "^2.0.5"
    },
    "dependencies": {
        "nango": "^0.42.9"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,cjs}": [
            "eslint --fix --quiet"
        ],
        "*.{html,css,json,yaml}": [
            "prettier --write"
        ]
    },
    "engines": {
        "node": ">=18.0.0 || >=20.0.0"
    }
}
