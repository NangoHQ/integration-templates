{
    "name": "integration-templates",
    "version": "1.0.0",
    "type": "module",
    "description": "",
    "scripts": {
        "cli": "npx tsx internal/scripts/zero/cli.ts",
        "build:flows": "npx tsx internal/scripts/zero/buildFlows.ts",
        "fix:zod-unions": "npx tsx internal/scripts/fix-zod-unions.ts",
        "lint": "eslint . --ext .ts",
        "fix": "eslint . --ext .ts --fix",
        "prettier-format": "prettier --config .prettierrc \"./**/*.{ts,tsx}\" --write",
        "prettier-watch": "onchange './**/*.{ts,tsx}' -- prettier --write {{changed}}",
        "prepare": "husky install",
        "resolve:aliases": "npx tsx internal/scripts/resolve-aliases.ts",
        "test": "vitest --run",
        "test:watch": "vitest"
    },
    "devDependencies": {
        "@nangohq/eslint-config": "1.0.6",
        "@nangohq/eslint-plugin-custom-integrations-linting": "0.0.4",
        "@nangohq/node": "^0.68.1",
        "@nangohq/providers": "^0.59.7",
        "@nangohq/shared": "^0.49.0",
        "@nangohq/types": "^0.58.3",
        "@types/ejs": "^3.1.2",
        "@types/js-yaml": "4.0.9",
        "@types/node": "20.12.2",
        "chalk": "5.4.1",
        "dotenv": "16.5.0",
        "ejs": "^3.1.10",
        "eslint": "8.56.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-import": "2.29.1",
        "eslint-plugin-prettier": "5.1.3",
        "husky": "8.0.3",
        "js-yaml": "4.1.0",
        "lint-staged": "15.2.9",
        "lodash-es": "^4.17.21",
        "nango": "^0.68.1",
        "onchange": "7.1.0",
        "parse-link-header": "^2.0.0",
        "soap": "1.1.2",
        "ts-to-zod": "^3.9.1",
        "tsx": "^4.19.0",
        "typescript": "5.7.2",
        "vitest": "^2.0.5",
        "zod": "4.0.5"
    },
    "lint-staged": {
        "*.{html,css,json,yaml,ts}": [
            "prettier --write"
        ]
    },
    "engines": {
        "node": ">=18.0.0 || >=20.0.0"
    }
}
